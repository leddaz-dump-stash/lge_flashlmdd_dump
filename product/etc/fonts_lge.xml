<?xml version="1.0" encoding="utf-8"?>
<!--
    Added System Default Fonts Family.
-->
<!-- Android Q familyset version update -->
<fonts-modification version='1'>
    <family customizationType='new-named-family' name="lgsm">
        <font weight="300" style="normal" postScriptName="LGSmHaL">LGSmHa_Light.ttf</font>
        <font weight="400" style="normal" postScriptName="LGSmHaR">LGSmHa_Regular.ttf</font>
        <font weight="700" style="normal" postScriptName="LGSmHaB">LGSmHa_Bold.ttf</font>
    </family>
    <family customizationType='new-named-family' name="lgkindgothic">
        <font weight="300" style="normal" postScriptName="HYKindGothic-Light">HYKindGothic_Light.ttf</font>
        <font weight="400" style="normal" postScriptName="HYKindGothic-Regular">HYKindGothic_Regular.ttf</font>
        <font weight="700" style="normal" postScriptName="HYKindGothic-Bold">HYKindGothic_Bold.ttf</font>
    </family>
    <family customizationType='new-named-family' name="lgfriend">
        <font weight="300" style="normal" postScriptName="HYFriend-Light">HYFriend_Light.ttf</font>
        <font weight="400" style="normal" postScriptName="HYFriend-Regular">HYFriend_Regular.ttf</font>
        <font weight="700" style="normal" postScriptName="HYFriend-Bold">HYFriend_Bold.ttf</font>
    </family>
    <family customizationType='new-named-family' name="lgtravel">
        <font weight="300" style="normal" postScriptName="HYTravel-Light">HYTravel_Light.ttf</font>
        <font weight="400" style="normal" postScriptName="HYTravel-Regular">HYTravel_Regular.ttf</font>
        <font weight="700" style="normal" postScriptName="HYTravel-Bold">HYTravel_Bold.ttf</font>
    </family>
    <family customizationType='new-named-family' name="lgyoyo">
        <font weight="300" style="normal" postScriptName="HYYoyo-Light">HYYoyo_Light.ttf</font>
        <font weight="400" style="normal" postScriptName="HYYoyo-Regular">HYYoyo_Regular.ttf</font>
        <font weight="700" style="normal" postScriptName="HYYoyo-Bold">HYYoyo_Bold.ttf</font>
    </family>

    <alias name="lgsm-light" to="lgsm" weight="300" />
    <alias name="lgkindgothic-light" to="lgkindgothic" weight="300" />
    <alias name="lgfriend-light" to="lgfriend" weight="300" />
    <alias name="lgtravel-light" to="lgtravel" weight="300" />
    <alias name="lgyoyo-light" to="lgyoyo" weight="300" />
</fonts-modification>
